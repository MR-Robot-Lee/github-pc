<div class="model-project-common">
    <div class="model-section">
        <div class="modal-form" style="margin-bottom: 20px">
            <label class="label-block int-small8"><i class="icon-fill-in"></i>部门:</label>
            <select class="int int-default" style="width: 364px" name="deptName">
                <option value="a">请选择</option>
            </select>
        </div>
        <div class="modal-form" style="margin-bottom: 20px">
            <label class="label-block int-small8"><i class="icon-fill-in"></i>职务:</label>
            <select class="int int-default" style="width: 364px" name="posName">
