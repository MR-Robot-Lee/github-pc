            <input type="password" class="int int-default" style="width: 364px" placeholder="请输入密码" name="pwd"
                   autocomplete="new-password">
        </div>
        <div class="modal-form" style="margin-bottom: 20px">
            <label class="label-block int-small8"><i class="icon-fill-in"></i>姓名:</label>
            <input type="text" class="int int-default" style="width: 364px" placeholder="请输入姓名" name="name">
        </div>
        <div class="modal-form" style="margin-bottom: 20px">
            <label class="label-block int-small8">性别:</label>
            <label> <input type="radio" name="sex" value="1">男</label>
            <label style="margin-left: 100px"> <input type="radio" name="sex" value="2">女</label>
        </div>
        <div class="modal-form" style="margin-bottom: 20px">
            <label class="label-block int-small8"><i class="icon-fill-in"></i>年龄:</label>
            <input type="text" class="int int-default" style="width: 364px" name="age" placeholder="请输入年龄">
        </div>
