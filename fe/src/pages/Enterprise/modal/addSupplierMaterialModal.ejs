<div class="add-supplier-in-meterial">
    <div>
        <div class="material-shift material-category supplier-item clearfix">
            <label>供应商类别 :</label>
            <!--<input class="add-supplier-ipt" type="text" name="" data-caveat="单位名称"-->
                   <!--placeholder="请输入供应商名称" />-->

            <div class="category-ipt">
                <input type="text" class="int int-default" name="" placeholder="输入类别名称" />
                <div class="back-to-select"><span class="icon-back-to-select"></span>取消</div>
            </div>
            <div class="int int-default category-sel" name="">
                <span>选择材料类别</span>
                <ul style="max-height: 300px;overflow: auto;">
                    <li class="add-new-type"><span class="icon-add-content"></span>新增类别</li>
                </ul>
                <div class="select-arrow"></div>
            </div>

        </div>
        <div class="supplier-item clearfix">
            <label>单位名称 :</label>
            <input class="add-supplier-ipt" type="text" name="entpName" data-caveat="单位名称"
                   placeholder="请输入单位名称" />
        </div>
        <div class="supplier-item clearfix">
            <label>联系人 :</label>
            <input class="add-supplier-ipt" type="text" name="contactName" data-caveat="联系人"
                   placeholder="请输入联系人" />
        </div>
        <div class="supplier-item clearfix">
            <label>电话 :</label>
            <input class="add-supplier-ipt" type="text" name="phone" data-caveat="电话"
                   placeholder="请输入电话" />
        </div>
        <div class="supplier-item clearfix">
            <label style="float: left">经营/工作范围 :</label>
            <textarea class="add-supplier-textarea" name="businessScope"
                      data-caveat="单位名称" placeholder="请输入单位名称"></textarea>
        </div>
        <div class="supplier-item clearfix">
            <label>税务类型 :</label>
            <select class="add-supplier-select" name="taxType">
                <option value="a">请选择</option>
                <option value="1">小规模</option>
                <option value="2">一般纳税人</option>
                <option value="3">个体</option>
            </select>
        </div>
        <div class="supplier-item clearfix">
            <label>开户名 :</label>
            <input class="add-supplier-ipt" type="text" name="openName" data-caveat="开户名"
                   placeholder="请输入开户名" />
        </div>
        <div class="supplier-item clearfix">
            <label>开户行 :</label>
            <input class="add-supplier-ipt" type="text" name="openBank" data-caveat="开户行"
                   placeholder="请输入开户行" />
        </div>
        <div class="supplier-item clearfix">
            <label>账号 :</label>
            <input class="add-supplier-ipt" type="text" name="bankCard" data-caveat="账号"
                   placeholder="请输入帐号" />
        </div>
        <div class="supplier-item clearfix">
            <label style="float: left;">附件 :</label>
            <div id="addSupplierAttach" style="display: inline-block;margin-left: 6px;margin-top: 5px;"></div>
        </div>
    </div>
    <div style="text-align: right;">
        <span class="span-btn confirm">确定</span>
        <span class="span-btn span-btn-bc confirm-active" data-model-close>取消</span>
    </div>
    <div class="triangle-left bebebe" style="top: 35px;left: -12px;"></div>
    <div class="triangle-left ffffff" style="top: 32px;left: -12px;"></div>
</div>