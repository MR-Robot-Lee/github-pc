<div class="model-project-common" style="width: 662px">
    <div class="modal-form" style="margin: 10px 20px;position: relative;">
        <div class="item">
            <label class="bold-title">项目部名称:</label>
            <input type="text" class="int int-default int-middle26" placeholder="请输入..." name="projDeptName">
        </div>
        <!--<div class="item">-->
            <!--<label class="bold-title">项目负责人:</label>-->
            <!--<div class="int int-default int-middle26" style="color: #999;display:inline-block;font-size: 12px;"-->
                 <!--id="chargeName">请选择...-->
            <!--</div>-->
        <!--</div>-->
        <div class="item">
            <label class="bold-title">项目成员及职务:</label>
            <span class="span-btn background-list-gray" id="selectEmployee">选择人员</span>
        </div>
        <div class="table-content" style="margin: 0;padding-bottom: 10px;height: 180px;overflow-y: auto;z-index: 50;">
            <table>
                <thead>
                <tr class="small">
                    <th class="border">姓名</th>
                    <th class="border">项目职务</th>
                    <th class="border">性别</th>
                    <th class="border">年龄</th>
