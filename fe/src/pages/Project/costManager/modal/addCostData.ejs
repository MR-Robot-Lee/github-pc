<div class="model-project-common" style="width: 340px;">
    <div class="model-section modal-form">
        <div style="margin-bottom: 20px;">
            <label class="label-block bold-title" style="width:60px;"><i style="vertical-align: middle" class="icon-fill-in">*</i>名称 :</label>
            <select class="int int-default" name="baseType">
                <option value="a">请选择</option>
                <option value="1">总产值</option>
                <option value="2">应收账款</option>
                <option value="3">实收账款</option>
            </select>
        </div>
        <div style="margin-bottom: 20px;">
            <label class="label-block bold-title" style="width:60px;"><i style="vertical-align: middle" class="icon-fill-in">*</i>时间 :</label>
            <input type="date" class="int int-default" name="addTime">
        </div>
        <div>
            <label class="label-block bold-title" style="width:60px;"><i style="vertical-align: middle" class="icon-fill-in">*</i>金额 :</label>
            <input type="text" class="int int-default" placeholder="请输入金额" name="outputMoney">
        </div>
    </div>
    <div class="model-footer">
        <span class="span-btn confirm">确认</span>
        <span class="span-btn span-btn-bc confirm-active" data-model-close>取消</span>
    </div>
</div>