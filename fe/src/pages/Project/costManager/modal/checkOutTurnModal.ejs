<div class="model-project-common">
    <div class="model-header">
        <span class="project-title">查看结算</span>
        <label style="font-size: 12px;margin-left: 20px">结算单生成时间:</label>
        <span style="font-size: 12px" class="project-time">- / - / -</span>
        <span class="span-btn background-list-gray historyList"
              style="position: absolute;right: 55px;top: 50%;margin-top: -15px;display: none;">查看</span>
        <span class="icon-close"></span>
    </div>
    <div class="model-section">
        <div class="modal-form" style="margin-bottom: 10px;text-align: center">
            <span class="bold-title item">结算管理(应付款)费用单</span>
        </div>
        <div class="table-content" style="margin: 0;">
            <table>
                <tbody>
                <tr class="small">
                    <td class="border" style="width: 75px;font-weight: bold;">分部名称</td>
                    <td class="border subProjName" colspan="3"></td>
                </tr>
                <tr class="small">
                    <td class="border" style="width: 75px;font-weight: bold;">费用名称</td>
                    <td class="border costName"></td>
                    <td class="border" style="width: 75px;font-weight: bold;">费用单号</td>
                    <td class="border costNo"></td>
                </tr>
                <tr class="small">
                    <td class="border" style="width: 75px;font-weight: bold;">结算单名称</td>
                    <td class="border settleName"></td>
                    <td class="border" style="width: 75px;font-weight: bold;">结算单编号</td>
                    <td class="border settleNo"></td>
                </tr>
                <tr class="small">
                    <td class="border" style="width: 75px;font-weight: bold;">费用生成人</td>
                    <td class="border settUserName"></td>
                    <td class="border" style="width: 75px;font-weight: bold;">结算金额</td>
                    <td class="border settlePrice"></td>
                </tr>
                <tr class="small">
                    <td class="border" style="width: 75px;font-weight: bold;">结算负责人</td>
                    <td class="border acctChargeName"></td>
                    <td class="border " style="width: 75px;font-weight: bold;">含税金额</td>
                    <td class="border taxMoney"></td>
                </tr>
                </tbody>
            </table>
        </div>
        <div id="contractInfo">
            <div class="modal-form" style="margin: 10px 0;text-align: center;">
                <span class="bold-title item">合同(约定)信息</span>
            </div>
            <div class="table-content" style="margin: 0;">
                <table>
                    <tbody>
                    <tr class="small">
                        <td class="border" style="width: 75px;font-weight: bold;">合同名称</td>
                        <td class="border cntrName" colspan="3"></td>
                        <td class="border" style="width: 75px;font-weight: bold;">合同编号</td>
                        <td class="border cntrNo" colspan="2"></td>
                    </tr>
                    <tr class="small">
                        <td class="border" style="width: 75px;font-weight: bold;">合同单位</td>
                        <td class="border entpName" colspan="3"></td>
                        <td class="border" style="width: 75px;font-weight: bold;">合作负责人</td>
                        <td class="border contactName" colspan="2"></td>
                    </tr>
                    <tr class="small">
                        <td class="border" style="width: 75px;font-weight: bold;">合同金额</td>
                        <td style="width: 120px;" class="border cntrPrice"></td>
                        <td class="border" style="width:40px;font-weight: bold;">含税</td>
                        <td class="border taxType" style="width: 35px;"></td>
                        <td class="border" style="width: 75px;font-weight: bold;">约定质保金</td>
                        <td class="border ensureMoney"></td>
                        <td class="border ensureMonth"></td>
                    </tr>
                    <tr class="small">
                        <td class="border" style="width: 75px;font-weight: bold;" rowspan="3">付款方式</td>
                        <td class="border payTypeDesc" colspan="6" rowspan="3"></td>
                        <!--<td class="border" style="width: 75px;font-weight: bold;">开户行</td>-->
                        <!--<td class="border openBank" colspan="2"></td>-->
                    </tr>
                    <tr class="small">
                        <!--<td class="border" style="width: 75px;font-weight: bold;">开户名</td>-->
                        <!--<td class="border openName" colspan="2"></td>-->
                    </tr>
                    <tr class="small">
                        <!--<td class="border" style="width: 75px;font-weight: bold;">账号</td>-->
                        <!--<td class="border bankCard" colspan="2"></td>-->
                    </tr>
                    </tbody>
                </table>
            </div>
        </div>
        <div class="modal-form" style="margin: 10px 0;text-align: center;">
            <span class="bold-title item">应付账款</span>
        </div>
        <div class="table-content" style="margin: 0;">
            <table>
                <tbody>
                <tr class="small">
                    <td class="border" rowspan="2" style="width: 75px;font-weight: bold;">预付款</td>
                    <td class="border payedMoney"></td>
                    <td class="border" style="width: 75px;font-weight: bold;">应付款金额</td>
                    <td class="border payableMoney"></td>
                </tr>
                <tr class="small">
                    <td class="border payedTaxMoney"></td>
                    <td class="border" style="width: 75px;font-weight: bold;">应提供税票</td>
                    <td class="border payableTaxMoney"></td>
                </tr>
                <tr class="small">
                    <td class="border" style="width: 75px;font-weight: bold;">暂扣质保金</td>
                    <td class="border ensurePayMoney"></td>
                    <td class="border" style="width: 75px;font-weight: bold;">质保期限</td>
                    <td class="border ensurePayMonth"></td>
                </tr>
                <tr class="small">
                    <td class="border" style="width: 75px;font-weight: bold;">备忘</td>
                    <td class="border remark" colspan="3"></td>
                </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="model-footer">
        <span class="span-btn span-btn-bc confirm-active" data-model-close>返回</span>
    </div>
</div>