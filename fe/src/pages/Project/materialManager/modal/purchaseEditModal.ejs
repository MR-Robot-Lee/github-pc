<!--采购编辑-->
<div class="model-project-common" style="width:1200px">
    <!-- <div class="modal-form" style="margin: 10px 20px;text-align: right">
        <span class="span-btn background-list-gray budgetSelect">选材</span>
     </div>-->
    <div class="table-content" style="height:374px;overflow-y: auto ">
        <table>
            <thead>
            <tr class="small">
                <th style="width: 45px" class="border">序号</th>
                <th class="border">材料名称</th>
                <th class="border">规格型号</th>
                <th class="border" style="width: 60px">单位</th>
                <th class="border" style="width: 60px">数量</th>
                <th class="border" style="width: 60px">单价</th>
                <th class="border" style="width: 70px;">合价</th>
                <th class="border">供货单位</th>
                <th class="border" style="width: 75px;">产地/品牌</th>
                <th class="border" style="width: 160px;">采购时间</th>
                <th class="border" style="width: 115px;">记账款项</th>
                <th class="border" style="width: 45px">含税</th>
                <th class="border" style="width: 45px">操作</th>
            </tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
    <div class="modal-form" style="margin: 10px 20px">
        <label class="bold-title">点收人:</label>
        <div class="int int-default int-small14 checkUserName"
             style="display: inline-block;vertical-align: middle;color: #333;line-height: 28px;width: 90px;">
            点击选择
        </div>
        <label style="margin-left: 15px;" class="bold-title">采购到场时间:</label>
        <input style="width: 160px; " type="date" class="int int-default int-small14 purchaseTime" />
    </div>
    <div class="model-footer">
        <span class="span-btn confirm">确认</span>
        <span class="span-btn span-btn-bc confirm-active" data-model-close>取消</span>
    </div>
</div>