<div id="costOrderContent" class="materialManagerOrder" style="">
    <div class="modal-form" style="padding: 0 20px">
        <div class="item" style="position: relative;padding: 20px 0">
            <div style="position: absolute;right: 0;top: 50%;margin-top: -15px;">
                <span class="span-btn background-list-gray" id="exportTable">导出</span>
            </div>
        </div>
        <div class="item">
            <label class="bold-title" style="font-size: 12px">费用生成人:</label>
            <input style="margin-right: 15px;width: 70px;" type="text" class="int int-default int-small11"
                   name="chargeUserName" disabled/>
            <label class="bold-title" style="font-size: 12px">费用生成总额:<span
                        style="position: absolute;left: 170px;top: 0;font-weight: normal;">元</span></label>
            <input style="margin-right: 15px;" type="text" class="int int-default int-small11" name="costMoney"
                   disabled/>
            <label class="bold-title" style="font-size: 12px">点收金额:<span
                        style="position: absolute;left: 145px;top: 0;font-weight: normal;">元</span></label>
            <input style="margin-right: 15px;" type="text" class="int int-default int-small11" name="costMoney"
                   disabled/>
            <label class="bold-title" style="font-size: 12px">费用生成时间:</label>
            <input style="margin-right: 15px;width: 150px;" type="text" class="int int-default int-small11" name="addTime"
                   disabled/>
            <label class="bold-title" style="font-size: 12px">未提交金额:</label>
            <input style="margin-right: 15px;width: 150px;" type="text" class="int int-default int-small11" name="unSubmitApprMoney"
                   disabled/>
        </div>
    </div>
    <div class="table-content"
         style="margin: 0 20px;">
        <table id="materialCostOrderTable">
            <thead id="showTitle">
            <tr class="small">
                <th style="width: 45px;" class="border">序号</th>
                <th class="border">费用编号</th>
                <th class="border">费用名称</th>
                <th class="border">分部名称</th>
                <th class="border">款项分类</th>
                <th class="border">总金额</th>
                <th class="border">含税金额</th>
                <th class="border">供货单位</th>
                <th class="border">预付金额</th>
                <th style="width: 60px;" class="border">状态</th>
                <th style="width: 70px;" class="border">操作</th>
            </tr>
            </thead>
            <thead id="hideTitle" style="position: absolute;z-index: 200;display: none;">
            <tr class="small"></tr>
            </thead>
            <tbody></tbody>
        </table>
    </div>
    <div style="text-align: right;
    margin: 10px 0px;
    padding: 10px 20px;
    border-top: 1px solid #e9e9e9;">
        <!-- <span class="span-btn span-btn-bc confirm-active">直传款项负责人</span>
         <span class="span-btn">保存</span>-->
        <span class="span-btn span-btn-bc confirm-active cancel">取消</span>
    </div>
</div>
<div id="costOrderNo" style="text-align:center;margin-top:100px;">
    <div class="icon-uninformed"></div>
    <div style="margin: 10px 0;color: #5b5b5b;">没有符合当前条件的编辑项</div>
    <div class="span-btn costOrder">
        <span class="icon-create"></span>
        <span>生成费用单</span>
    </div>
</div>
