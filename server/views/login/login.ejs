<div class="login-content">
    <div class="login-des">个人登录</div>
    <div class="login-user">
        <span class="login-icon"></span>
        <input class="login-int" type="text" placeholder="请输入您的手机号码/帐号" name="mobile" autocomplete=off/>
    </div>
    <div class="login-pwd">
        <span class="login-icon pwd"></span>
        <input class="login-int" type="password" placeholder="请输入您的密码" name="pwd" autocomplete=off/>
    </div>
    <div class="login-reset">
        <label><input type="checkbox" id="rememberKey"><span style="position: relative;top: -2px;">记住密码</span></label>
        <a href="/login/password" class="reset-pwd confirm-hover">忘记密码?</a>
    </div>
    <div class="login-btn" id="login">登录</div>
</div>
