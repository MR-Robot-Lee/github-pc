<%- include('../../layout/header') %>
<div class="IndexMain page-company-setting">
    <%- include('../systemHeader') %>
    <div class="container margin-58">
        <div class="span-left" style="width: 260px;">
            <%- include('./nav') %>
        </div>
        <div class="span-right" style="margin-left: 280px;">
            <div class="container-list">
                <div class="common-header word">
                    <div class="search-form">
                        <% if(type === 'system-index'){ %>
                        <span class="common-title">企业信息</span>
                        <% }else if(type === 'contact'){ %>
                        <span class="common-title">联系人信息</span>
                        <% }else if(type === 'billboard'){ %>
                        <span class="common-title">宣传栏更新</span>
                        <% } %>
                    </div>
                </div>
                <div class="container-list-warp">
                    <% if(type === 'system-index'){ %>
                    <%- include('./companyInfo') %>
                    <% }else if(type === 'contact'){ %>
                    <%- include('./contactInfo') %>
                    <% }else if(type === 'billboard'){ %>
                    <%- include('./billboardUpdate') %>
                    <% } %>
                </div>
            </div>
        </div>
    </div>
    <div class='model-wrap'></div>
</div>
<script type="text/javascript" src="/dist/system.js"></script>
<%- include('../../layout/footer') %>
