<div class="bidnotice-item-container">
    <div style="display: inline-block;vertical-align: middle;margin-top: 20px;padding-left: 10px;">
        <label style="margin-left:10px;">类型名称 :</label>
        <select class="int int-default int-small14" name="bidType">
            <option value="">请选择</option>
            <option value="1">材料</option>
            <option value="2">人工</option>
            <option value="3">措施</option>
            <option value="4">分包</option>
            <option value="5">企业服务</option>
        </select>
        <label style="margin-left:10px;">项目名称 :</label>
        <select class="int int-default int-small14" name="allProject" style="width: 200px;">
            <option value="">请选择</option>
        </select>
        <input style="padding-left: 5px;margin-left: 10px;" type="text" class=" int int-default keywords" placeholder="请输入关键字">
        <span style="margin-top: -4px;line-height: 30px;margin-left: 7px;" class="span-btn" id="searchModal">查询</span>
    </div>
    <div name='noInfoBidNoticeList_main' class="bidnotice-item-list" style="padding: 0 15px;">
    </div>
</div>
<div name="noInfoBidNoticeList_page" id="page" style="margin: 20px 0;display: none;"></div>
<div class='noInfo' id='noInfoBidNoticeList'>
    <div class="icon-uninformed"></div>
    <p>没有符合条件的信息！</p>
</div>

<div class="bidnotice-item-detail" style="display: none;">
    <div class="detail-contain" style="border: none ;">
        <div class="clearfix detail-top">
            <div class="fl detail-title"></div>
        </div>
        <div class="clearfix detail-info">
            <div class="fl"><label>招标编号 : </label><span style="color: #5b5b5b;" class="bidNo"></span></div>
            <div class="fl"><label>定标人 : </label><span style="color: #5b5b5b;" class='winBidUserName'></span></div>
            <div class="fl"><label>定标时间 : </label><span style="color: #5b5b5b;" class='winBidTime'></span></div>
        </div>
        <div class="detail-content" style="padding-top: 15px;">
            <div class="clearfix" style="margin-bottom: 10px;">
                <div class="fl" style="margin-right: 40px;"><label>招标类型 : </label><span style="color: #5b5b5b;" class="bidType"></span></div>
                <div class="fl"><label>项目名称 : </label><span style="color: #5b5b5b;" class="projectName"></span></div>
            </div>
            <div style="margin-bottom: 10px;"><label>中标单位 : </label><span style="color: #5b5b5b;" class="entpName"></span></div>
            <div style="margin-bottom: 10px;"><label>投标总价 : </label><span style="color: #5b5b5b;" class="biddingMoney"></span>元</div>
            <div class="clearfix" style="margin-bottom: 20px;">
                <div class="fl">
                    <label>招标要求 :</label>
                </div>
                <div class="table-content fl" style="width: 800px;margin: 0 0 0 5px;">
                    <table>
                        <thead>
                        <tr class="small">
                            <th class="border" style="width: 45px;">序号</th>
                            <th class="border">内容</th>
                        </tr>
                        </thead>
                        <tbody id="bidRequireList"></tbody>
                    </table>
                </div>
            </div>
            <div class="clearfix" style="margin-bottom: 20px;">
                <div class="fl">
                    <label>招标清单 :</label>
                </div>
                <div class="table-content fl" style="width: 800px;margin: 0 0 0 5px;">
                    <table>
                        <thead>
                        <tr class="small">
                            <th class="border" style="width: 45px;">序号</th>
                            <th class="border">项目分类</th>
                            <th class="border">项目名称</th>
                            <th class="border">工作内容</th>
                            <th class="border">单位</th>
                            <th class="border">数量</th>
                            <th class="border">说明</th>
                        </tr>
                        </thead>
                        <tbody id="bidDetailVOList"></tbody>
                    </table>
                </div>
            </div>
            <!--<div class="clearfix">-->
                <!--<label class="fl" style="margin-left: 30px;">设置 : </label>-->
                <!--<div class="fl">-->
                    <!--<input type="checkbox" disabled checked>-->
                    <!--<span>平台所有人可见</span>-->
                <!--</div>-->
            <!--</div>-->
        </div>
    </div>
</div>

