<!--列表展示页-->
<div class="bid-item-list">
    <div style="display: inline-block;vertical-align: middle;margin-top: 20px;padding-left: 10px;">
        <label style="margin-left:10px;">类型名称 :</label>
        <select class="int int-default int-small14" name="bidType">
            <option value="">全部类型</option>
            <option value="1">材料</option>
            <option value="2">人工</option>
            <option value="3">措施</option>
            <option value="4">分包</option>
            <option value="5">企业服务</option>
        </select>
        <label style="margin-left:10px;">项目名称 :</label>
        <select class="int int-default int-small14" name="allProject" style="width: 200px;">
            <option value="">全部项目</option>
        </select>
        <label style="margin-left:10px;">状态名称 :</label>
        <select class="int int-default int-small14" name="bidStatus" style="width: 200px;">
            <option value="">全部状态</option>
            <option value="1">待发布</option>
            <option value="2">招标中</option>
            <option value="3">已截止</option>
            <option value="4">流标</option>
            <option value="7">已评标</option>
            <option value="8">被撤回</option>
        </select>
        <input style="padding-left: 5px;margin-left: 10px;" type="text" class="int int-default keywords" placeholder="请输入关键字">
        <span style="margin-top: -4px;line-height: 30px;margin-left: 7px;" class="span-btn" id="searchModal">查询</span>
    </div>
    <div name="noInfoBidsList_main" class="all-bids" style="padding: 0 15px;">
    </div>
</div>
