<%- include('../../layout/projectHeader') %>
<div class='IndexMain container-project' id="projectSchedule" data-id="<%- id %>" data-type="<%- sType %>">
    <div class='project-menu-title'><span><%- name %></span>
        <div style="position: absolute;top: 5px;left: 25px;width: 36px;height: 36px;border-radius: 50%;box-shadow: 0px 0px 2px 1px #c1c1c1;background-color: #fff;"><img style="width: 28px;height: 25px;border-radius: 50%;margin-top: 6px;" name='projectPrincipalLogo' src="./../../../dist/images/my-apply.png" alt=""></div>
        <div id="projectPrincipal" style="position: absolute;top: 13px;left: 70px;font-size: 14px"></div>
    </div>
    <div class="container-content-project page-budget">
        <%- include('./../projectDetailManager/projectNav') %>
        <div class="project-list">
            <div class="budget-title">进度管理</div>
            <div class="budget-menus">
                <% if(sType === 1){ %>
                <a class="item active" href="/project/schedule/<%- id %>?name=<%- name %>">
                    <span class="menu-line"></span>
                    主进度计划
                </a>
                <% } else { %>
                <a class="item" href="/project/schedule/<%- id %>?name=<%- name %>">
                    <span class="menu-line"></span>
                    主进度计划
                </a>
                <% } %>
                <% if(sType === 2){ %>
                <a class="item active" href="/project/schedule/child/<%- id %>?name=<%- name %>">
                    <span class="menu-line"></span>
                    次进度计划
                </a>
                <% } else { %>
                <a class="item" href="/project/schedule/child/<%- id %>?name=<%- name %>">
                    <span class="menu-line"></span>
                    次进度计划
                </a>
                <% } %>
            </div>
            <div class="project-list-wrap top-88">
                <div id="scheduleShow" style="display: none">

                    <div style="margin-top: 10px;padding-bottom: 10px;position: relative;border-bottom: 1px solid #e9e9e9">
                        <% if(sType === 2){ %>
                            <div class="span-btn addSchedule" style="margin-left: 20px">
                                <span class="icon-create"></span>
                                <span>新建计划</span>
                            </div>
                            <div id="selectSchedule" style="display: inline-block;min-width: 270px;vertical-align: middle; position: relative;z-index: 200;margin-left: 10px;"></div>
                        <% } %>
                        <span class="span-btn background-9441" id='editSchedule' style="margin-left: 20px">进度修改</span>
                        <span class="span-btn background-9441" id='stopSchedule'>中止计划</span>
                    </div>
                    <div class="page-schedule">
                        <div class="schedule-title span" name="schedName"></div>
                        <div class="schedule-content">
                            <label class="bold-title">计划编制人:</label>
                            <span class="span userName" style="display: inline-block;width: 100px;"></span>
                            <label class="bold-title margin-left">进度负责人:</label>
                            <span class="span" name="chargeUserName" style="display: inline-block;width: 100px;"></span>
                            <label class="bold-title margin-left">执行状态:</label>
                            <span class="span" name="schedStatus" style="display: inline-block;width: 100px;"></span>
                            <label class="bold-title margin-left">进度评估:</label>
                        <span class="span span-color-blue" name="judgeType"
                              style="display: inline-block;width: 100px;"></span>
                            <label class="bold-title margin-left">计划整体进度:</label>
                            <div style="width: 100px;display: inline-block" id="barItem"></div>
                        </div>
                        <dvi class="schedule-content">
                            <label class="bold-title">开竣工日期:</label>
                            自<span class="span span-color-blue" name="planBeginTime"
                                   style="display: inline-block;width: 110px;margin-left: 5px;"></span>
                            至<span class="span span-color-blue" name="planEndTime"
                                   style="display: inline-block;width: 110px;margin-left: 5px;"></span>
                            <label style="margin-left: 30px;" class="bold-title margin-left">合同工期:</label>
                        <span class="span span-color-blue" name="cntrDays" id="typeDate"
                              style="display: inline-block;width: 100px;"></span>
                            <label style="margin-left: 33px;" class="bold-title margin-left">计划工期:</label>
                        <span class="span span-color-blue" name="planDays"
                              style="display: inline-block;width: 100px;"></span>
                            <label class="bold-title margin-left">计划工日:</label>
                        <span class="span span-color-blue" name="planWorkDays"
                              style="display: inline-block;width: 100px;"></span>
                        </dvi>
                        <div class="table-content" style="margin: 10px 10px;">
                            <table>
                                <thead>
                                <tr>
                                    <th style="width: 45px;" class="border">序号</th>
                                    <th class="border">项目名称</th>
                                    <th class="border">责任人</th>
                                    <th style="width: 120px;" class="border">计划工期(天)</th>
                                    <th style="width: 120px;" class="border">每天人数(人)</th>
                                    <th class="border">计划开始时间</th>
                                    <th class="border">计划完成时间</th>
                                    <th class="border">完成情况</th>
                                    <th class="border">进度评估</th>
                                    <th class="border" style="width: 120px;">操作</th>
                                </tr>
                                </thead>
                                <tbody id="scheduleManager"></tbody>
                            </table>
                        </div>
                    </div>
                </div>
                <div id="scheduleHide" style="text-align: center;margin-top: 100px;display: none;">
                    <div class="icon-uninformed"></div>
                    <div style="margin: 10px 0;color: #5b5b5b;">没有符合条件的进度计划,快去新建计划吧</div>
                    <div class="span-btn addSchedule" style="margin-left: 20px">
                        <span class="icon-create"></span>
                        <span>新建计划</span>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class='model-wrap'></div>
</div>
<script type="text/javascript" src="/dist/project.js"></script>
<%- include('../../layout/footer') %>
