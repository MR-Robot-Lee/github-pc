<%- include('../../layout/projectHeader') %>
<div class='IndexMain container-project page-material ' id="projectSchedule"
     data-id="<%- id %>">
    <div class='project-menu-title'><span><%- name %></span>
        <div style="position: absolute;top: 5px;left: 25px;width: 36px;height: 36px;border-radius: 50%;box-shadow: 0px 0px 2px 1px #c1c1c1;background-color: #fff;"><img style="width: 28px;height: 25px;border-radius: 50%;margin-top: 6px;" name='projectPrincipalLogo' src="./../../../dist/images/my-apply.png" alt=""></div>
        <div id="projectPrincipal" style="position: absolute;top: 13px;left: 70px;font-size: 14px"></div>
    </div>
    <div class="container-content-project page-budget">
        <%- include('./../projectDetailManager/projectNav') %>
        <div class="project-list">
            <div class="budget-title">结算管理</div>
            <div class="budget-menus">
                <% if(type === 'balance-index' || type === 'balance-contract'){ %>
                <a class="item active" href="/project/balance/index/<%- id %>?name=<%- name %>">
                    <span class="menu-line"></span>
                    结算
                </a>
                <% }else{ %>
                <a class="item " href="/project/balance/index/<%- id %>?name=<%- name %>">
                    <span class="menu-line"></span>
                    结算
                </a>
                <% } %>
                <% if(type === 'balance-resource'){ %>
                <a class="item active" href="/project/balance/resource/<%- id %>?name=<%- name %>">
                    <span class="menu-line"></span>
                    结算(合同)资源
                </a>
                <% }else{ %>
                <a class="item" href="/project/balance/resource/<%- id %>?name=<%- name %>">
                    <span class="menu-line"></span>
                    结算(合同)资源
                </a>
                <% } %>
            </div>
            <div class="project-list-wrap top-88">
                <% if(type === 'balance-index'){ %>
                <%- include('./balanceIndex') %>
                <% }else if(type === 'balance-resource'){ %>
                <%- include('./contractBalanceResource') %>
                <% }else if (type === 'balance-contract'){ %>
                <%- include('./noContractBalance') %>
                <% }else if (type === 'balance-check'){ %>
                <%- include('./noContractBalanceCheck') %>
                <% } %>
            </div>
            <% if(type === 'balance-index'){ %>
            <%- include('./contractBalanceDetail') %>
            <% } %>
        </div>
    </div>
    <div class='model-wrap'></div>
</div>
<script type="text/javascript" src="/dist/project.js"></script>
<%- include('../../layout/footer') %>
