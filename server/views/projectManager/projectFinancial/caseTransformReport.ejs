<!--成本结转报告-->
<div class="modal-form" style="padding:10px 0;border-bottom: 1px solid #e9e9e9;position: relative;">
    <span class="bold-title">成本结转报告</span>
    <div style="position: absolute;top: 50%;margin-top: -15px;right: 20px;">
        <span class="span-btn background-list-gray" id="updateAccounts">决算更新</span>
    </div>
</div>
<div style="border-bottom: none" class="cost-assess-content">
    <div class="modal-form border-bottom">
        <div class="item">
            <label class="bold-title"><%- name %></label>
            <label style="margin-left: 20px;font-size: 12px">报告生成时间:</label>
            <span style="font-size: 12px;color: #5b5b5b;" id="currentDate"></span>
        </div>
        <div class="item">
            <label class="bold-title">合同金额:</label>
            <span class="span-color-blue" id="cntrMoney"></span>
            <span class="span-color-blue">元</span>
            <label class="bold-title" style="margin-left: 20px">结算金额:</label>
            <span class="span-color-blue" id="settleMoney"></span>
            <span class="span-color-blue">元</span>
            <label class="bold-title" style="margin-left: 20px">毛利润:</label>
            <span class="span-color-blue" id="grossProfitMoney"></span>
            <span class="span-color-blue">元</span>
            <label class="bold-title" style="margin-left: 20px">利润率:</label>
            <span class="span-color-blue" id="grossProfitMoneyPer"></span>
        </div>
    </div>
    <div style="border-bottom: 1px solid #e9e9e9;height: 280px;min-width: 1080px;">
        <div class="cost-income-item border-right" style="height: 280px;width: 520px;">
            <div class="modal-form">
                <div class="item bold-title">项目费用构成</div>
            </div>
            <div class="table-content">
                <table>
                    <thead>
                    <tr class="small">
                        <th class="border" style="width: 70px;">费用构成</th>
                        <th class="border" style="width: 80px;">金额</th>
                        <th class="border" style="width: 90px;">占总价款(%)</th>
                    </tr>
                    </thead>
                    <tbody id="profitDom"></tbody>
                </table>
            </div>
            <div class="costIncome" id="costIncome" style="width: 220px;"></div>
        </div>
        <div class="cost-income-item">
            <div class="modal-form">
                <div class="item bold-title" style="margin-left: 10px;">工程(分部)构成</div>
            </div>
            <div class="table-content" style="padding-left: 10px;height: 220px;overflow-y: auto;">
                <table>
                    <thead>
                    <tr class="small">
                        <th class="border" style="width: 120px;">分部名称</th>
                        <th class="border" style="width: 68px;">结算金额</th>
                        <th class="border" style="width: 59px;">占总(%)</th>
                    </tr>
                    </thead>
                    <tbody id="divisionProjectTable"></tbody>
                </table>
            </div>
            <div class="costIncome" id="costContract" style="width: 220px;"></div>
        </div>
    </div>
    <div class="clr"></div>
    <div class="modal-form" style="margin-top: 10px;">
        <label class="bold-title">分部财务状况<span>(计划值与实际值)</span></label>
    </div>
    <div class="table-content">
        <table style="table-layout: fixed;">
            <thead>
            <tr>
                <th style="width: 136px;">分部名称</th>
                <th style="width: 136px;">利润率</th>
                <th style="width: 136px;">材料费率</th>
                <th style="width: 136px;">人工费率</th>
                <th style="width: 136px;">措施费率</th>
                <th style="width: 136px;">分包费率</th>
            </tr>
            </thead>
            <tbody id="costDivisionTable"></tbody>
        </table>
    </div>
