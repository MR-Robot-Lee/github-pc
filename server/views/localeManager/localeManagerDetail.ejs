<div data-id="<%- id %>" data-type="<%- navType %>" class="locale-detail border" id="locale-detail"
     data-time="<%- time %>">
    <div class="locale-detail-title"></div>
    <div class="locale-detail-attr">
        <div class="clearfix">
            <div style="float: left;width: 190px;">
                <label>现场管理 :</label>
                <span class="projectName"></span>
            </div>
            <div style="float: left;width: 190px;margin-left: 70px;">
                <label>项目部 :</label>
                <span class="projDeptName"></span>
            </div>
        </div>
        <div class="clearfix">
            <div style="float: left;width: 190px;">
                <label>发布时间 :</label>
                <span class="addTime"></span>
            </div>
            <div style="float: left;width: 190px;margin-left: 70px;">
                <label>编辑人 :</label>
                <span class="userName"></span>
            </div>
        </div>
    </div>
    <div class="local-detail-content"></div>
    <div class="local-detail-images"></div>
</div>
<div class="locale-detail" id="pageTotal"></div>