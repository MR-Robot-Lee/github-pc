<div class="modal-form" style="margin: 10px 20px;font-size: 12px" id="materialHistoryId" data-hid="<%- id %>">
    <div class="bold-title"></div>
    <div style="margin-top: 10px">
        <span style="font-size: 14px;color: #333333;font-weight: bold" name="mtrlName"></span>
    </div>
    <div style="margin-top: 10px">
        <label class="bold-title bold-title-enterprise">材料类别 :</label>
        <span name="mtrlCategory" style="display: inline-block"></span>
        <label style="margin-left: 30px;" class="bold-title bold-title-enterprise">材料类型 :</label>
        <span name="mtrlType" style="display: inline-block;"></span>
    </div>
    <div style="margin-top: 10px">
        <label class="bold-title bold-title-enterprise">规格型号 :</label>
        <span name="specBrand"></span>
    </div>
    <div style="margin-top: 10px;">
        <label class="bold-title  bold-title-enterprise">计量单位 :</label>
        <span name="unit" style="display: inline-block"></span>
        <label style="margin-left: 30px;" class="bold-title bold-title-enterprise">平均价格 :</label>
        <span class="span-color-blue" name="avgPrice" style="display: inline-block"></span>
        <label style="margin-left: 30px;" class="bold-title bold-title-enterprise">合计金额 :</label>
        <span class="span-color-blue" name="totalMoney"></span>
    </div>
</div>
<div class="table-content">
    <table>
        <thead>
        <tr class="small">
            <th style="color: #5b5b5b;" class="border">序号</th>
            <th style="color: #5b5b5b;" class="border">工程简称</th>
            <th style="color: #5b5b5b;" class="border">材料所用分部</th>
            <th style="color: #5b5b5b;" class="border">采购时间</th>
            <th style="color: #5b5b5b;" class="border">采购人</th>
            <th style="color: #5b5b5b;" class="border">数量</th>
            <th style="color: #5b5b5b;" class="border">单价</th>
            <th style="color: #5b5b5b;" class="border">是否含税</th>
            <!--<th class="border">价格采用</th>-->
            <th style="color: #5b5b5b;" class="border">供货单位</th>
            <th style="color: #5b5b5b;" class="border">产地/品牌</th>
        </tr>
        </thead>
        <tbody id="materialHistory"></tbody>
    </table>
</div>
<div id="page" style="margin: 20px 0;"></div>
<!--<input type="checkbox" disabled>-->